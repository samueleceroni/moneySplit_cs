<Window x:Class="GUI.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="LoginWindow" Height="107" Width="187">
    <Grid>
        <TextBox x:Name="loginIDTextBox" HorizontalAlignment="Left" Height="23" Margin="23,11,0,0" TextWrapping="Wrap" TextAlignment="Center" Text="Enter ID" VerticalAlignment="Top" Width="64"/>
        <Button x:Name="loginButton" Content="Login" HorizontalAlignment="Left" VerticalAlignment="Top" Width="64" Margin="92,11,0,0" Height="23"/>
        <Button x:Name="registerUserButton" Content="Register User" HorizontalAlignment="Left" VerticalAlignment="Top" Width="133" Margin="23,39,0,0" Height="23"/>

    </Grid>
</Window>
